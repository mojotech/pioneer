{
  "name": "dill",
  "description": "Delicious Cucumber tests",
  "keywords": [
    "cucumber",
    "selenium-webdriver",
    "testing"
  ],
  "author": "MojoTech",
  "version": "0.4.0",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "main": "./lib/dill",
  "bin": {
    "dill": "./bin/dill"
  },
  "scripts": {
    "build": "npm run-script clean && gulp",
    "clean": "rm -rf lib",
    "publish": "npm run-script build && npm publish && npm run-script clean",
    "test": "mocha test/unit/**.coffee",
    "integration": "./bin/dill test/integration/features --require test/integration/steps --require test/integration/widgets --format=pretty --driver=phantomjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mojotech/dill.js.git"
  },
  "devDependencies": {
    "coffee-script": "1.7.1",
    "mocha": "^1.19.0",
    "sinon": "^1.10.0",
    "should": "^3.3.1",
    "bluebird": "^1.2.4",
    "gulp-include": "samccone/gulp-include",
    "gulp": "^3.6.2",
    "gulp-coffee": "^1.4.3"
  },
  "dependencies": {
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0",
    "cucumber": "~0.3.1",
    "lodash": "~2.2.1",
    "moment": "^2.6.0",
    "optimist": "^0.6.1",
    "selenium-webdriver": "2.41.0"
  }
}
